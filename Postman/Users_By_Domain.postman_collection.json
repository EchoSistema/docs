      "item": [
        {
          "name": "Update User (Profile)",
          "request": {
            "method": "PUT",
            "header": [
              { "key": "Authorization", "value": "Bearer {{token}}" },
              { "key": "X-PUBLIC-KEY", "value": "{{public_key}}" },
              { "key": "Accept-Language", "value": "{{language}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/v1/ia/admin/users/{{user}}",
              "host": ["{{baseUrl}}"],
              "path": ["api","v1","ia","admin","users","{{user}}"]
            },
            "body": {"mode":"raw","raw":"{\n  \"name\": \"Jane Doe\"\n}"}
          }
